@model List<ResultPriceWithCarDto>
<section class="ftco-section ftco-cart">
    <div class="container">
        <div class="row">
            <div class="col-md-12 ftco-animate">
                <div class="car-list">
                    <table class="table">
                        <thead class="thead-primary">
                            <tr class="text-center">
                                <th>&nbsp;</th>
                                <th>&nbsp;</th>
                                <th class="bg-primary heading">Saatlik Kiralama</th>
                                <th class="bg-dark heading">Günlük Kiralama</th>
                                <th class="bg-black heading">Aylık Kiralama</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach(var item in Model)
                            {
                            <tr class="">
                                <td class="car-image"><div class="img" style="background-image:url(/carbook-master/images/@item.carImageUrl);"></div></td>
                                <td class="product-name">
                                    <h3>@item.carName</h3>
                                    <p class="mb-0 rated">
                                        <span>rated:</span>
                                        <span class="ion-ios-star"></span>
                                        <span class="ion-ios-star"></span>
                                        <span class="ion-ios-star"></span>
                                        <span class="ion-ios-star"></span>
                                        <span class="ion-ios-star"></span>
                                    </p>
                                </td>

                                <td class="price">
                                    <p class="btn-custom"><a href="#">Araba Kirala</a></p>
                                    <div class="price-rate">
                                        <h3>
                                            <span class="num"><small class="currency">₺</small> @item.amountHour</span>
                                            <span class="per">/saatlik</span>
                                        </h3>
                                        <span class="subheading"></span>
                                    </div>
                                </td>

                                <td class="price">
                                    <p class="btn-custom"><a href="#">Araba Kirala</a></p>
                                    <div class="price-rate">
                                        <h3>
                                            <span class="num"><small class="currency">₺</small> @item.amountDay</span>
                                            <span class="per">/günlük</span>
                                        </h3>
                                        <span class="subheading"></span>
                                    </div>
                                </td>

                                <td class="price">
                                    <p class="btn-custom"><a href="#">Araba Kirala</a></p>
                                    <div class="price-rate">
                                        <h3>
                                            <span class="num"><small class="currency">₺</small> @item.amountWeek</span>
                                            <span class="per">/haftalık</span>
                                        </h3>
                                        <span class="subheading"></span>
                                    </div>
                                </td>
                            </tr>
                            }
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>